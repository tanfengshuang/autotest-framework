<?xml version="1.0" encoding="UTF-8"?>
<virtlab>
    <job name="KVM-acceptance_loop-big" component="kvm" product="RHEV" need_reinstall="True" need_multiple_machine="True">
        <machine profile="RHEL5-U4-x86_64-kvm-smp-minimal">intel-5504-12-1.englab.nay.redhat.com</machine>
        <case name="ConfigureSoakTest">
            <parameter name="guestname">Linux</parameter>
            <parameter name="platform">all</parameter>
            <parameter name="category">acceptance_loop</parameter>
            <parameter name="nicmodel">virtio_nic</parameter>
            <parameter name="driveformat">virtio_blk</parameter>
            <parameter name="hosttype">big</parameter>
            <parameter name="ksm">1</parameter>
            <parameter name="imageformat">raw</parameter>
            <parameter name="snapshot">no</parameter>
            <parameter name="rhev">no</parameter>
            <parameter name="network">bridge</parameter>
            <parameter name="bridge">switch</parameter>
            <parameter name="mig_timeout">200</parameter>
        </case>
    </job>
</virtlab>
